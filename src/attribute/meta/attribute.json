{
    "attribute": {
        "use": ["attribute-base", "attribute-complex"],
        "submodules": {
            "attribute-core": {
                "requires": [
                    "oop"
                ]
            },
            "attribute-extras": {
                "requires": [
                    "oop"
                ]
            },
            "attribute-observable": {
                "requires": [
                    "eventx",
                    "eventx-compat"
                ]
            },
            "attribute-base": {
                "requires": [
                    "attribute-core",
                    "attribute-observable",
                    "attribute-extras"
                ]
            },
            "attribute-complex": {
                "requires": [
                    "attribute-base"
                ]
            },
            "attribute-events": {
                "use": ["attribute-observable"]
            }
        }
    }
}
